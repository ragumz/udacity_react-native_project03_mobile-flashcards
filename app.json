{
  "expo": {
    "name": "Flash Cards",
    "description": "Organize your studies and have fun",
    "slug": "mobile_flashcards",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "packagerOpts": {
      "port": 19003
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brainstorm.flashcards.exp"
    },
    "android": {
      "package": "com.brainstorm.flashcards.exp"
    }
  }
}